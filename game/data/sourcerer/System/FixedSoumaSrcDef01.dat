		if(aau[i].isPutItem())
		{
			clusMul.setFocusUnit(aau[i]);
		}
	}
	
	while(!clusBase.evokeOriginalElements()){}
	while(!clusMul.evokeOriginalElements()){}

	clusBase.dissolve();
	clusMul.dissolve();
	clusBase.synthesize("motol", clusMul);
